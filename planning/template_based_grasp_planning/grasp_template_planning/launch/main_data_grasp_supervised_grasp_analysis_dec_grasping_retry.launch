<launch>
	<arg name="robot" default="armrobot"/>
	
	<node pkg="grasp_template_planning" type="main_data_grasp_supervised_grasp_analysis" name="main_grasp_data_supervised_grasp_analysis" output="screen">
		<param name="dir_path_bagfiles" type="str" value="$(find grasp_template_planning)/data/log_archive/grasping_experiments_data/Experiments_Dec/grasping_with_retry_failures"/>
		<param name="dir_path_database" type="str" value="$(find grasp_template_planning)/data/deep_learning/grasp-database/"/>
		<param name="dir_path_dataset" type="str" value="$(find grasp_template_planning)/data/deep_learning/grasp-dataset-supervised-dec-grasping-retry/"/>
		<param name="database_name" type="str" value="arm-grasp"/>
		<param name="dataset_name" type="str" value="grasping-with-retry-failures"/>
		<rosparam file="$(find grasp_template_planning)/config/template_config_$(arg robot).yaml" command="load"/>
	</node>
</launch>
